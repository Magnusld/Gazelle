<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefWidth="500" styleClass="courseList" stylesheets="@../stylesheets/main.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gazelle.ui.CourseListController">
   <children>
      <HBox alignment="CENTER_LEFT">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Mine Løp">
               <font>
                  <Font size="22.0" />
               </font>
            </Text>
            <Region HBox.hgrow="ALWAYS" />
              <Button fx:id="newCourse" mnemonicParsing="false" onAction="#handleNewCourseClick" styleClass="buttonNew" text="Nytt Løp">
               <font>
                  <Font size="14.0" />
               </font></Button>
         </children>
      </HBox>
      <Pane styleClass="separator">
         <VBox.margin>
            <Insets bottom="10.0" top="10.0" />
         </VBox.margin>
      </Pane>
      <VBox fx:id="courseList" spacing="10.0" />
   </children>
</VBox>
