<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gazelle.ui.CourseListController">
   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="50.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Mine Løp">
               <font>
                  <Font size="22.0" />
               </font>
            </Text>
            <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
              <Button fx:id="newCourse" mnemonicParsing="false" onAction="#handleNewCourseClick" prefHeight="20.0" prefWidth="95.0" style="-fx-background-color: #8AC4FF;" text="Nytt Løp" />
         </children>
      </HBox>
      <Pane maxHeight="1.0" minHeight="1.0" prefHeight="1.0" style="-fx-background-color: lightgray;">
         <VBox.margin>
            <Insets bottom="10.0" top="10.0" />
         </VBox.margin>
      </Pane>
      <VBox fx:id="courseList" prefHeight="400.0" prefWidth="471.0" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
