<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefWidth="500" styleClass="list" stylesheets="@../stylesheets/main.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER_LEFT">
         <children>
            <Text fx:id="title" strokeType="OUTSIDE" strokeWidth="0.0" text="Title">
               <font>
                  <Font size="22.0" />
               </font>
            </Text>
            <Region HBox.hgrow="ALWAYS" />
              <Button fx:id="editButton" minWidth="120.0" mnemonicParsing="false" onAction="#onEditButton" text="Rediger">
               <font>
                  <Font size="14.0" />
               </font></Button>
         </children>
      </HBox>
      <Pane styleClass="separator">
         <VBox.margin>
            <Insets bottom="10.0" top="10.0" />
         </VBox.margin>
      </Pane>
      <Text fx:id="description" strokeType="OUTSIDE" strokeWidth="0.0" text="Beskrivelse" />
      <VBox fx:id="list" spacing="10.0" />
   </children>
</VBox>
